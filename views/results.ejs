<%- include('partials/header.ejs') %>
    <h1>Cool Images</h1>
    <body>
        <form action="/search">
            <input type="text" placeholder="Type keyword" name="keyword"/>
            <br /><br />
            <button>Search</button>
            <br /><a id="favLink" href="/getKeywords"> See Favorites </a>
        </form>
        <hr />
        <div id="resultsContainer">
            <% for (let i = 0; i < 9; i++) { %>
                <%- (i % 3 == 0) ? '<br />': '' %>
                <img src='<%= imageUrlArray[i] %>' width='200' height='200' />
                <img class="favortiteIcon" src='img/favorite.png' width='20' />
            <% } %>
        </div>
<%- include('partials/footer.ejs') %>        