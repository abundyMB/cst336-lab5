<%- include('partials/header.ejs') %>
    <body>
        
        <main>
            
            <h1>Cool Images</h1>
            <form action="/search">
                <input type="text" placeholder="Type keyword" name="keyword"/>
                <br /><br />
                <button>Search</button>
            </form>
            <hr />
            <% for (let i = 0; i < 9; i++) { %>
                <%- (i % 3 == 0) ? '<br />': '' %>
                <img src='<%= imageUrlArray[i] %>' width='200' height='200' />
                <img class="favortiteIcon" src='img/favorite.png' width='20' />
           <% } %>
            
            
        </main>
        
<%- include('partials/footer.ejs') %>        